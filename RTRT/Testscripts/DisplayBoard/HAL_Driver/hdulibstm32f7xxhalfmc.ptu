                VAR device.BWTR[0],  init = ( T_UINT32)0,          ev = ( T_UINT32)65809
            
            COMMENT OUTPUT SECTION:
            
                --Holds the HAL status
                VAR ret_FmcNorSramExtendedTimingInit,     
                &                    init = HAL_ERROR,                      ev = HAL_OK
           
            SIMUL
            
            --No stubs
            
            END SIMUL

        -- Calling the Unit Under Test :
        #ret_FmcNorSramExtendedTimingInit = FmcNorSramExtendedTimingInit(&device, &timing, bank, extended_mode);
        
    END ELEMENT

