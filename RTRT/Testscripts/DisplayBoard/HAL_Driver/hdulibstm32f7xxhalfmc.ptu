            COMMENT INPUT SECTION:
            
                --Holds the value of extended mode
                VAR extended_mode,      init = FMC_EXTENDED_MODE_ENABLE,    ev = init
            
                --Holds the member values of NORSRAM timing structure
                STRUCTURE timing,       init = { AddressSetupTime => 1, AddressHoldTime => 1, DataSetupTime => 1, 
                &                                BusTurnAroundDuration => 1, CLKDivision => 1, DataLatency => 1, AccessMode => 1 },    
                &                                                           ev = init
            
                --Holds the value of bank number
                VAR bank,               init = 0,                           ev = init

            COMMENT INPUT/OUTPUT SECTION:
            
