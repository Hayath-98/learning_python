SERVICE DemoInit
SERVICE_TYPE extern

    --Local variables Declarations
    #T_OS_EVENT PTU_OsSemCreate;
    
    #T_UINT32 Loop_index, Loop_index_1=0;

TEST 7
FAMILY nominal

	COMMENT Test Description
	COMMENT <TD>   
	COMMENT [hdudispfdemo-DoDemo-DB-LLR-006]
	COMMENT The function 'DoDemo' loops from zero to MAX_A429_CHANNEL-1 and zero to MAX_A429_LABELS-1 
	COMMENT and does the following:
	COMMENT - Sets doc of BID of sID of transmit message to (result of addition of DEMO_DOC_ID and 
	COMMENT   channel index value) masked with DOC_MSK
	COMMENT - Sets demo label to label of  A429_data of A429 Demo data with indices channel value and label count.
	COMMENT - If eng_sdi of Config_data is lessthan to ONE
	COMMENT  	- Sets the SDI to result of channel plus one	
	COMMENT - Sets the data to data of A429 Demo data with indices channel value and label count.
	COMMENT - Increments the localtimer of A429 Demo data with indices channel value and label count by one.
	COMMENT -    Loop from ZERO to Timer_cnt and do nothing ( 0 loop)
	COMMENT Equivalence Class 79	
	COMMENT
	COMMENT [hdudispfdemo-DoDemo-DB-LLR-009]
	COMMENT The function 'DoDemo' loops from zero to MAX_A429_CHANNEL-1 and zero to MAX_A429_LABELS-1 
	COMMENT and does the following 
	COMMENT 1.Does the following when type of A429_data of A429 Demo data with indices channel 
	COMMENT   value and label count is equal to DDW and
	COMMENT  Demo label is equal to Exceedance label 
  	COMMENT  a. Resets the data of A429 Demo data data with indices channel value and label count 
  	COMMENT     when data value is equal to MAX_EXCEE_DATA when function loops through 22 and above( from 15th iteration of 22).
  	COMMENT  b.	Builds the data value by left shifting the data value by TEN.
  	COMMENT  a. does nothing when 
  	COMMENT     data value is lesser than MAX_EXCEE_DATA. in 1 to 22 iteration(till 14th of 22nd iteration)  	
	COMMENT Equivalence Class 15,34,32,14,22
	COMMENT </TD> 


	ELEMENT

		COMMENT INPUT SECTION:
		
			VAR Demo_mode,						init = TRUE,							ev = init
		
			VAR Timer_cnt,						init = 0,								ev = init			

			VAR Demo_mode_activate,				init = TRUE,							ev = init

			VAR A429_settings_cnt,				init = 256,								ev = init
			
			VAR Config_data.eng_sdi,			init = ZERO,							ev = init
			
			VAR A429_cfg_data[0..255].A429_port,		init = ONE,								ev = init
			
			VAR A429_cfg_data[0..255].A429_type,		init = DDW,								ev = init
			
			VAR A429_cfg_data[0..255].A429_label,	init = 1,								ev = init
			
			VAR Cas_msg[0].label,				init = 0,								ev = init

			VAR Exceed_data[0].label,			init = 1,								ev = init

			VAR Cas_cnt,						init = 255,								ev = init
			

			VAR A429_cfg_data[0..255].label_data.A429_bnr_data.A429_bnr_bits,		
			&											init = 1,								ev = init 

			VAR A429_cfg_data[0..255].label_data.A429_bnr_data.A429_bnr_scale_fact,
			&											init = 1.0,								ev = init 
			
			
		COMMENT INPUT/OUTPUT SECTION:

			VAR U16_sys_info_words[1],			init = 2,								ev = 2

		COMMENT OUTPUT SECTION:
		
			--Allocate stack for the Demo task
			VAR Demo_task_stk[0..1023],			init = (T_OS_STK)0xFFFFFFFF,			ev = 0xDEADDEAD
			
			--Sem to signal task execution
			VAR Sem_demo_task,					init = NULL,							ev = &PTU_OsSemCreate
			
			VAR Demo_active,					init = FALSE,							ev = TRUE

		--Stub Calls:
		SIMUL
		
			--Creates and initializes the Semaphore
			STUB OsSemCreate 1..674 => (ZERO)(&PTU_OsSemCreate)
			
			--Creates OS Task
			STUB OsTaskCreate 1..674 => (NULL, &Demo_task_stk[DEMO_TASK_STK_SIZE], DEMO_TASK_PRIO)1
			
			--Function waits on a Semaphore
			STUB OsSemPend 1..2022 => (&PTU_OsSemCreate, 0)
			
			--Configures the can
			STUB CAN1DemoConfig()
			
			--Builds a CAN message from the user supplied ARINC 825 message
			STUB A8251Xmit 1..32683 => ()A825_XM_OK
		
		END SIMUL

		-- Calling the Unit Under Test is called for 8 times to increment act_delay,
		-- 666 times to increment localtimer
		
		#for(Loop_index=0;Loop_index<674;Loop_index++)
		#{
			# A429_cfg_data[Loop_index_1].A429_type = BNR;  	 		
		    #A429DemoInit();
			# A429_cfg_data[Loop_index_1].A429_type = DDW; 
			# Loop_index_1++;
			#if(Loop_index_1 >=256)Loop_index_1 = 0;
			#DemoInit();
		#}
	
	END ELEMENT
	
END TEST -- TEST 7

