            VAR PTU_error,                                        init = 0x1,                   ev = 0x0

            --variable used in stub OsTaskCreate
            VAR PTU_pt_os,                                        init = 0x1,                   ev = &A429_task_stk[A429_TASK_STK_SIZE]

        COMMENT INPUT/OUTPUT SECTION:
                
